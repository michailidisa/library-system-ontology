###  http://example.org/library#AcademicStaff
lib:AcademicStaff rdf:type owl:Class ;
                  owl:equivalentClass lib:Faculty ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty lib:pAcWorkOnDept ;
                                        owl:someValuesFrom lib:Department
                                      ] ;
                  rdfs:subClassOf lib:User ;
                  owl:disjointWith lib:AdministrativeStaff ,
                                   lib:External ;
                  owl:disjointUnionOf ( lib:AssistantProfessor
                                        lib:AssociateProfessor
                                        lib:Professor
                                      ) ;
                  owl:hasKey ( lib:pAcRegNo
                             ) .


###  http://example.org/library#AdministrativeStaff
lib:AdministrativeStaff rdf:type owl:Class ;
                        rdfs:subClassOf lib:Person ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty lib:pAddHasDirectSupervisor ;
                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass lib:AdministrativeStaff
                                        ] .


###  http://example.org/library#AssistantProfessor
lib:AssistantProfessor rdf:type owl:Class ;
                       rdfs:subClassOf lib:AcademicStaff ,
                                       lib:Author .


###  http://example.org/library#AssociateProfessor
lib:AssociateProfessor rdf:type owl:Class ;
                       rdfs:subClassOf lib:AcademicStaff ,
                                       lib:Author .


###  http://example.org/library#Author
lib:Author rdf:type owl:Class ;
           rdfs:subClassOf lib:Person ;
           owl:hasKey ( lib:pAuthorId
                      ) .


###  http://example.org/library#Book
lib:Book rdf:type owl:Class ;
         rdfs:subClassOf lib:Document .


###  http://example.org/library#Copy
lib:Copy rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty lib:isCopyOf ;
                           owl:allValuesFrom lib:Document
                         ] ;
         owl:hasKey ( lib:cId
                    ) .


###  http://example.org/library#Department
lib:Department rdf:type owl:Class .


###  http://example.org/library#Document
